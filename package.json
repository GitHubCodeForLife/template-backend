{
  "name": "node-demo",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "test": "mocha './{,!(node_modules)/**}/*.test.mjs' --reporter mochawesome --reporter-options autoOpen=true --experimental-modules --es-module-specifier-resolution=node --timeout 10000 && exit 1",
    "clean": "rm -rf dist && mkdir dist",
    "build-babel": "node_modules/babel/bin/babel.js src --source-maps --out-dir dist",
    "build": "npm run clean && npm run build-babel",
    "production": "node dist/application/server.js",
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/application/server.js",
    "dev": "nodemon --experimental-modules --es-module-specifier-resolution=node src/application/server.js",
    "devs": "nodemon --exec ./node_modules/.bin/babel-node src/application/server.js",
    "start-prod": "set NODE_ENV=production && node ./src/application/server.js",
    "pre-commit": "lint-staged",
    "prepare": "husky install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@hapi/joi": "^15.1.1",
    "auto-bind": "^5.0.1",
    "bcrypt": "^5.0.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.2",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "cloudinary": "^1.29.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^10.0.0",
    "express": "^4.17.3",
    "express-session": "^1.17.2",
    "hbs": "^4.2.0",
    "http-errors": "~1.6.3",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mocha": "^9.2.2",
    "mochawesome": "^7.1.3",
    "mongoose": "^6.3.3",
    "morgan": "~1.9.1",
    "multer": "^1.4.4",
    "nodemailer": "^6.7.3",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "passport": "^0.5.2",
    "passport-facebook": "^3.0.0",
    "passport-google": "^0.3.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "rimraf": "^3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.9",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "babel": "^5.8.29",
    "eslint": "^8.15.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "husky": "^8.0.1",
    "lint-staged": "^12.4.1",
    "nodemon": "^2.0.15"
  },
  "lint-staged": {
    "*.+(js|cjs)": [
      "eslint --fix"
    ]
  }
}
